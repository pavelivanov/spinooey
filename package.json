{
  "name": "spinooey",
  "version": "0.0.1",
  "description": "A collection of single element css loaders and spinners with gooey effect",
  "homepage": "https://github.com/grammka/spinooey#readme",
  "author": "Pavel Ivanov (grammka@gmail.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grammka/spinooey.git"
  },
  "keywords": [
    "spinooey",
    "gooey",
    "spinners",
    "spinner",
    "loaders",
    "loading",
    "stylus",
    "css",
    "ui"
  ],
  "main": "./lib",
  "scripts": {
    "start": "mkdirp ./lib && npm run build",
    "prepublish": "npm run build",
    "clean": "rimraf ./lib",
    "dev": "stylus -w ./src/spinooey.styl -o ./lib -u ./node_modules/nib/lib/nib",
    "build": "stylus ./src/spinooey.styl -o ./lib -u ./node_modules/nib/lib/nib"
  },
  "dependencies": {},
  "devDependencies": {
    "mkdirp": "^0.5.1",
    "nib": "^1.1.2",
    "rimraf": "^2.5.4",
    "stylus": "^0.54.5"
  }
}
